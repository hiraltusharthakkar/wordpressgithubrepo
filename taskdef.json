{
    "executionRoleArn": "arn:aws:iam::054037131762:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "New",
            "image": "054037131762.dkr.ecr.eu-central-1.amazonaws.com/project/newregistryecr:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
